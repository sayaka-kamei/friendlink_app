<h3>以下の内容で、送信する。</h3>

<%= form_with(model: @picture, pictures_path, local: true) do |form| %>
  <%= form.hidden_field :title %>
  <%= form.hidden_field :content %>

  <p>画像:<%= @picture.image %></p>
  <p>本文:<%= @picture.content %></p>
  <%= form.submit  "登録する" %>

  <%= link_to '戻る', name: 'back'  %>

<% end %>